            android:textStyle="bold"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Blue Dream Real Estate Ltd."
            android:textSize="23sp"
            android:textColor="@android:color/black" />





    </soup.neumorphism.NeumorphCardView>


    <soup.neumorphism.NeumorphCardView
        android:clickable="true"
        android:focusable="true"
        android:layout_marginTop="30sp"
        android:id="@+id/Asian"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="16dp"
        app:neumorph_lightSource="leftTop"
        app:neumorph_shadowElevation="5dp"
        app:neumorph_shapeType="flat"
        app:neumorph_backgroundColor="@android:color/white"
        app:neumorph_cornerFamily="rounded"
        app:neumorph_cornerSize="16dp"
        tools:ignore="MissingConstraints"
        app:neumorph_shadowColorDark="@color/purple_500"
        app:neumorph_shadowColorLight="@color/black"

        >

        <TextView
            android:layout_gravity="center"
            android:gravity="center"
            android:textStyle="bold"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Asian Life Foundation."
            android:textSize="23sp"
            android:textColor="@android:color/black" />



    </soup.neumorphism.NeumorphCardView>






</LinearLayout>